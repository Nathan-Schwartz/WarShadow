{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"WarShadow",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"Nathan Schwartz",
		"icon":"images/IconMouseOver.png",
		"icon_gray":"images/IconMouseNormal.png",
		"description":"Demo App"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"html/main.html",
					"transparent":true,
					"resizable":true,
					"size":{"width":65, "height":50},
					"min_size":{"width":25, "height":25},
					"max_size":{"width":300, "height":800}
					
			},
			"HSCounter":{
				"file":"html/HSCounter.html",
				"transparent":true,
				"resizable":true,
				"size":{"width":140, "height":50},
				"min_size":{"width":25, "height":25},
				"max_size":{"width":300, "height":500},
				"show_in_taskbar": false
			},
			"HSPercent":{
				"file":"html/HSPercent.html",
				"transparent":true,
				"resizable":true,
				"size":{"width":140, "height":50},
				"min_size":{"width":25, "height":25},
				"max_size":{"width":300, "height":500},
				"show_in_taskbar": false
			},
				
				
			"SmokeTimer":{
				"file":"html/SmokeTimer.html",
				"transparent":true,
				"resizable":true,
				"size":{"width":100, "height":60},
				"min_size":{"width":25, "height":25},
				"max_size":{"width":300, "height":500},
				"show_in_taskbar": false
			},
				
			"StatCrack":{
				"file":"html/StatCrack.html",
				"transparent":false,
				"resizable":true,
				"size": {"width": 1000, "height": 800},
				"start_position": {"Top": 100,"Left": 100}
			},

			"HSChains":{
				"file":"html/HSChains.html",
				"transparent":true,
				"resizable":true,
				"size":{"width":140, "height":50},
				"min_size":{"width":25, "height":25},
				"max_size":{"width":300, "height":500},
				"show_in_taskbar": false
			},
			"Info":{
				"file":"html/Info.html",
				"transparent":false,
				"resizable":true,
				"show_in_taskbar": true,
				"size": {"width": 800, "height": 600},
				"start_position": {"Top": 100,"Left": 300}
			},	
			
			"Crosshair":{
				"file":"html/crosshair.html",
				"transparent":true,
				"resizable":true,
				"size":{"width":200, "height":200},
				"min_size":{"width":25, "height":25},
				"max_size":{"width":300, "height":500},
				"show_in_taskbar": false
			},
			
			"Settings":{
					"file":"html/settings.html",
					"transparent":false,
					"resizable":true,
					"size":{"width":400, "height":600}
			},
			
			"KDR":{
				"file":"html/KDR.html",
				"transparent":true,
				"resizable":true,
				"size":{"width":140, "height":50},
				"min_size":{"width":25, "height":25},
				"max_size":{"width":300, "height":500},
				"show_in_taskbar": false
			}
		},
		
		"hotkeys" : {
			"crosshair": {
				"title": "Crosshair Hide",
				"action-type": "custom",
				"default": "0"
			}
		},
		
		
		
		"plugins": [
			"npIOPlugin.dll"
		]
		
		
	}
}